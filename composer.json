{
    "name": "codeqasim/php-headless-cms",
    "description": "A modern PHP headless CMS with drag-and-drop API creation",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "catfan/medoo": "^2.1",
        "codeqasim/mailer": "^1.0",
        "qaxim/php-router": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "secure-http": false
    }
}
